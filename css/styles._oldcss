  transition: color 0.2s, border-color 0.2s, background-color 0.2s;
}
/* Mixins */
.i-block,
.iblock {
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: top;
  *zoom: 1;
  *display: inline;
  margin-right: -4px;
}
.i-block-mid,
.iblockmid {
  display: -moz-inline-stack;
  display: inline-block;
  vertical-align: middle;
  *zoom: 1;
  *display: inline;
  margin-right: -4px;
}
.uper {
  text-transform: uppercase;
  letter-spacing: 1px;
}
.textCenter {
  display: block;
  text-align: center;
}
.positionCenter {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  max-width: 100%;
  max-height: auto;
  margin: auto;
}
.after {
  content: ' ';
  display: block;
  position: absolute;
}
.trn {
  -o-transition: all 0.35s;
  -ms-transition: all 0.35s;
  -moz-transition: all 0.35s;
  -webkit-transition: all 0.35s;
  transition: all 0.35s;
}
