}
.textCenter {
  display: block;
  text-align: center;
}
.positionCenter {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  max-width: 100%;
  max-height: auto;
  margin: auto;
}
.after {
  content: ' ';
  display: block;
  position: absolute;
}
.trn {
  -o-transition: all 0.35s;
  -ms-transition: all 0.35s;
  -moz-transition: all 0.35s;
  -webkit-transition: all 0.35s;
  transition: all 0.35s;
}
