.prj-more:after {position: absolute; background: url(../img/logos/i-load.png) no-repeat left; width:30px; height:23px; display:block; content:' '; top:-6px; left:-30px;}
.prj-more:hover {color:#e15243; border-bottom-color: rgba(225, 83, 67, 0.3);}



.srch {display: inline-block;vertical-align: top;*zoom: 1;*display: inline; width:260px; margin-right:5px; position: relative; border:1px solid #e7e7e7; height:33px;}
.srch-input {position: absolute; top:0; left:0; width:90%; height:100%; color:#626262; font-size:13px; text-indent: 15px;}
.srch-button {position: absolute; right:0; top:0; height:100%; width:13%; z-index:4; background: url(../img/logos/i-search.png) no-repeat center; cursor: pointer;}
.srch-button:hover {background-color: #f6f6f4;}


.select {width:260px; display: inline-block;vertical-align: top;*zoom: 1;*display: inline; margin-right:5px; position: relative; height:33px; border:1px solid #e7e7e7; background: url(../img/logos/i-arrow.png) no-repeat right 10px center; cursor: pointer; z-index:8;}
.select-val {font-size:13px; color:#000; padding-left:15px; margin-right:25px; line-height:35px; white-space: nowrap; overflow: hidden;}
.select-list {background: #ffffff; box-shadow: 0 10px 26px rgba(0,0,0,0.15); z-index:7; font-family: 'PT Sans', sans-serif; font-size:12px; max-height:250px; overflow: hidden; display:none;}
.select-list li {padding:6px 15px;}
.select-list li:hover {color:#fff; background: #1796ca;}
.scrollpane {}

.add {background: #f6f6f4; height:35px; display: inline-block;vertical-align: top;*zoom: 1;*display: inline; width:260px; position: relative; font-size:13px; color:#000; text-align: left; text-indent: 15px; line-height:35px; cursor: pointer;}
.add:after {content:'+'; color:#1996ca; position: absolute; font-size:18px; right:10px; top:0; line-height:35px; font-weight: bold;}


.button .hidden {opacity: 0 !important; display:block; position: absolute; top:0; left:0; width:100%; height:100%; cursor: pointer !important;}


 .rightcol-ar {display:none;}

.w7590 .rightcol {display:block; width:100%; position: fixed; bottom:0; background: #f4f4f4; overflow: hidden; height:55px; padding:0; left:0; cursor: pointer;}
.w7590 .rightcol-ar {position: absolute; display:block; width:13px; height:8px; background: url(../img/lol.png) no-repeat; right:20px; top:25px; display:block;}
.w7590 .rightcol h3 {padding-top:12px; padding-left:20px;}

.w7590 .rightcol-content {margin: 20px;}